{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach Debugger",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Debug: fetch_gw2_skills",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/fetch_gw2_skills.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: get_skill_enum",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_skill_enum.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: get_skill_mapping",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_skill_mapping.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: get_html_logs",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_html_logs.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: get_html_logs (Recent)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_html_logs.py",
            "args": ["--recent_only"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: get_html_logs (skip)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_html_logs.py",
            "args": ["--skip_metadata"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: html_log_to_json",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/html_log_to_json.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: MANUAL get_html_logs",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_html_logs.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": ["--manual"],
            "justMyCode": false
        },
        {
            "name": "Debug: version script",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/create_version_file.py",
            "args": [
                "--output=C:/Users/schaf.JANPC/Documents/_LocalCoding/GW2_RotaHelper/data"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Image to C Array",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/img_to_c_array.py",
            "args": [
                "resources/TEX_GW2RotaHelperNORMAL.png",
                "resources/TEX_GW2RotaHelperHOVER.png",
                "--array-name1",
                "ARR_GW2RotaHelperNORMAL",
                "--array-name2",
                "ARR_GW2RotaHelperHOVER"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true
        }
    ]
}
