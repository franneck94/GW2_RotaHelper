{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach Debugger",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Debug: Get Benches V4",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/get_benches_v4.py",
            "args": ["--output", "data/bench"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: Visit Benches",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/visit_all_beches.py",
            "args": ["--output", "data/html"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Debug: Get Skills List",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/fetch_gw2_skills.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },

    ]
}
